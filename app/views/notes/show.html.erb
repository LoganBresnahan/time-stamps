<h2>
  <%= @note.title %>
</h2>
<p>
  <%= link_to :edit, edit_note_path(@note) %>
</p>

<div>
  <% @note.videos.each do |video| %>
    <%= video.title %>
    <%= video.url %>
  <% end %>
</div>

<div>
  <%= render '/videos/new' %>
</div>

<div>
  <%= render '/stamps/new' %>
</div>

<div>
  <% @note.stamps.each do |stamp| %>
    <%= stamp.time %>
    <%= stamp.body %>
  <% end %>
</div>
