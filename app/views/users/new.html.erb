<h3>Register</h3>

<%= form_for :user, url: users_path do |f| %>
  <div>
    <%= f.label :username %>
    <%= f.text_field :username, placeholder: 'username' %>
  </div>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email, placeholder: 'email' %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password, placeholder: 'password' %>
  </div>
  <div>
    <%= f.submit :register %>
  </div>
<% end %>
